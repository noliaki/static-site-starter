{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape"],
    "no-unused-variable": true,
    "no-unused-expression": true,
    "one-variable-per-declaration": [true, "ignore-for-loop"]
  },
  "rulesDirectory": []
}
