{
  "name": "template-single-page",
  "version": "1.0.0",
  "description": "template for single web page",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:noliaki/template-single-page.git",
    "type": "git"
  },
  "author": "noliaki <noliaki.mobile@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"webpack-tsconfig.json\" run-s clean",
    "build": "cross-env NODE_ENV=production run-s clean start:build:production",
    "clean": "ts-node scripts/clean.ts",
    "start:build:production": "run-s webpack run-prod",
    "start:build:dev": "run-p webpack run-dev",
    "webpack": "cross-env TS_NODE_PROJECT=\"webpack-tsconfig.json\" webpack",
    "run-dev": "node ./scripts/dev.js",
    "run-prod": "node ./scripts/production.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@noliaki/eslint-config-typescript-prettier": "^0.1.0",
    "@noliaki/prettier-config": "0.0.1",
    "@types/autoprefixer": "^9.6.1",
    "@types/browser-sync": "^2.26.1",
    "@types/chalk": "^2.2.0",
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/imagemin-pngquant": "^7.0.0",
    "@types/imagemin-svgo": "^7.0.0",
    "@types/node": "^13.1.7",
    "@types/nunjucks": "^3.1.3",
    "@types/rimraf": "^2.0.3",
    "@types/stylus": "^0.48.32",
    "@types/webpack": "^4.41.2",
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.26.7",
    "chalk": "^3.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "fast-glob": "^3.1.1",
    "htmlhint": "^0.11.0",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.0",
    "postcss": "^7.0.26",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0",
    "stylus": "^0.54.7",
    "terser-webpack-plugin": "^2.3.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
