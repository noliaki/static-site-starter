{
  "name": "template-single-page",
  "version": "1.0.0",
  "description": "template for single web page",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:noliaki/template-single-page.git",
    "type": "git"
  },
  "author": "noliaki <noliaki.mobile@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-s clean start:build:dev",
    "build": "cross-env NODE_ENV=production run-s clean start:build:production",
    "clean": "node scripts/clean.js",
    "start:build:production": "run-s webpack run-prod",
    "start:build:dev": "run-p webpack run-dev",
    "webpack": "webpack --config ./webpack.config.js",
    "run-dev": "node ./scripts/dev.js",
    "run-prod": "node ./scripts/production.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@noliaki/eslint-config-prettier": "^0.2.0",
    "@noliaki/prettier-config": "^0.0.1",
    "@types/lodash": "^4.14.137",
    "autoprefixer": "^9.6.1",
    "browser-sync": "^2.26.7",
    "cross-env": "^6.0.0",
    "eslint": "^6.2.2",
    "fast-glob": "^3.0.4",
    "fs-extra": "^8.1.0",
    "htmlhint": "^0.11.0",
    "imagemin": "^7.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.22",
    "prettier": "^1.18.2",
    "pug": "^2.0.4",
    "rimraf": "^3.0.0",
    "stylus": "^0.54.7",
    "terser-webpack-plugin": "^2.0.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "lint-staged": "^9.2.4"
  }
}
