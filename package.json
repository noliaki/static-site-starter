{
  "name": "template-single-page",
  "version": "1.0.0",
  "description": "template for single web page",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:noliaki/template-single-page.git",
    "type": "git"
  },
  "author": "noliaki <noliaki.mobile@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-s clean start:build:dev",
    "build": "cross-env NODE_ENV=production run-s clean start:build:production",
    "clean": "node scripts/clean.js",
    "start:build:production": "run-s webpack run-prod",
    "start:build:dev": "run-p webpack run-dev",
    "webpack": "webpack --config ./webpack.config.js",
    "run-dev": "node ./scripts/dev.js",
    "run-prod": "node ./scripts/production.js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie >= 11"
  ],
  "dependencies": {
    "@noliaki/eslint-config-prettier": "^0.0.2",
    "@noliaki/prettier-config": "^0.0.1",
    "@noliaki/tslint-config-prettier": "^0.0.1",
    "@types/lodash": "^4.14.119",
    "autoprefixer": "^9.4.3",
    "browser-sync": "^2.24.4",
    "cross-env": "^5.1.6",
    "eslint": "^5.11.1",
    "fast-glob": "^2.2.4",
    "fs-extra": "^7.0.0",
    "htmlhint": "^0.10.1",
    "imagemin": "^6.1.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-pngquant": "^6.0.1",
    "imagemin-svgo": "^7.0.0",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.7",
    "prettier": "^1.15.3",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5",
    "terser-webpack-plugin": "^1.2.1",
    "ts-loader": "^5.3.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.0.0"
  },
  "devDependencies": {}
}
